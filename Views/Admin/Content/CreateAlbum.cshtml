@model MangoTaikaDistrict.Domain.Entities.Album
<h2>Créer album</h2>

<form method="post">
    <div>Titre: <input asp-for="Titre" /></div>
    <div>Visibilité:
        <select asp-for="Visibilite">
            <option value="PUBLIC">Public</option>
            <option value="PRIVE">Privé</option>
        </select>
    </div>
    <button type="submit">Créer</button>
</form>

<p><a asp-area="Admin" asp-controller="Content" asp-action="Albums">Retour</a></p>
